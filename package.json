{
  "name": "regl-scatterplot",
  "version": "0.4.0",
  "description": "WebGL-based 2D Scatterplot in Regl",
  "author": "Fritz Lekschas",
  "keywords": [
    "scatterplot",
    "2d",
    "regl",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/regl-scatterplot.js",
  "module": "src/index.js",
  "files": [
    "src/*",
    "dist/regl-scatterplot.js",
    "dist/regl-scatterplot.min.js"
  ],
  "scripts": {
    "build": "rollup -c; webpack --config ./webpack.config.js --mode production",
    "ghp": "npm run build; rm -rf ../regl-scatterplot-ghp/*; touch ../regl-scatterplot-ghp/.nojekyll; cp -r docs/* ../regl-scatterplot-ghp/",
    "lint": "eslint src tests rollup.config.js webpack.config.js",
    "precommit": "NODE_ENV=production lint-staged; npm run test",
    "prepublishOnly": "npm run test",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "test": "rollup -c ./rollup.test.config.js | tape-run --render='tap-spec'",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "camera-2d-simple": "^2.0.0-rc2",
    "canvas-camera-2d": "^0.5.5",
    "gl-matrix": "^2.8.1",
    "is-key-down": "^0.1.0",
    "kdbush": "^3.0.0",
    "lodash-es": "^4.17.11",
    "pub-sub-es": "^1.2.0",
    "regl": "^1.3.9",
    "regl-line": "^0.1.2",
    "scroll-speed": "flekschas/scroll-speed",
    "with-raf": "^1.0.0"
  },
  "peerDependencies": {
    "pub-sub-es": "^1.2.0",
    "regl": "^1.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "acorn": "^6.0.4",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "browser-env": "^3.2.5",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^v3.3.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "esm": "^3.0.84",
    "gl": "^4.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.1.0",
    "merge": ">=1.2.1",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "rollup": "^0.67.3",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "tap-spec": "^5.0.0",
    "tape-run": "^4.0.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-tape-run": "0.0.7",
    "zora": "^2.0.1"
  },
  "homepage": "https://github.com/flekschas/regl-scatterplot",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/regl-scatterplot.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/regl-scatterplot/issues"
  }
}
